    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "debug test",
            "cargo": {
                "args": [
                    "test",
                    "exprlist",
                    "--no-run"
                ],
                "filter": {
                    "name": "parser"
                }
            }
        }
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rua'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rua",
                    "--package=rua"
                ],
                "filter": {
                    "name": "rua",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rua'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rua",
                    "--package=rua"
                ],
                "filter": {
                    "name": "rua",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}